{
  "manifest_version": 2,
  "name": "Instension",
  "version": "0.0.0",
  "background": {
    "scripts": ["scripts/background.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://*.instagram.com/*"],
      "js": ["scripts/element_finder.js", "scripts/content.js"]
    }
  ],
  "permissions": ["contextMenus", "downloads"]
}
